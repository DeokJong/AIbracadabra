{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext", // import.meta 지원 모듈 시스템
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["vite/client","unplugin-vue-router/client","vite-plugin-vue-layouts/client"] // import.meta.env 타입 정의
  },
  "include": ["env.d.ts", "src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue","typed-router.d.ts"]
}
